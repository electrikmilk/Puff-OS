{
  "name": "Terminal",
  "version": "1.0.0",
  "about": "Basic terminal app",
  "sbin": true,
  "menus": {
    "Tools": {
      "Console": {
        "onclick": "apps.open('Console')"
      },
      "Audio Manager": {
        "onclick": "apps.open('Audio Manager')"
      },
      "Process Manager": {
        "onclick": "apps.open('Process Manager')"
      }
    },
    "Quick Commands": {
      "Refresh toolbar": {
        "id": "applist"
      },
      "Check network": {
        "id": "net"
      },
      "View processes": {
        "id": "top"
      },
      "Clear past output": {
        "id": "clear"
      },
      "Show all commands": {
        "id": "help"
      }
    },
    "Demos": {
      "Not a virus": {
        "onclick": "apps.open('Not a virus')"
      },
      "Dialogs": {
        "onclick": "apps.open('Dialogs')"
      },
      "UI Gallery": {
        "onclick": "apps.open('UI Gallery')"
      }
    }
  }
}