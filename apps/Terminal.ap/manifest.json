{
  "name": "Terminal",
  "version": "1.0.0",
  "about": "Basic terminal app",
  "sbin": true,
  "menus": {
    "Quick": {
      "Refresh taskbar": {
        "id": "applist"
      },
      "Check network": {
        "id": "net"
      },
      "View processes": {
        "id": "top"
      },
      "Clear past output": {
        "id": "clear"
      },
      "Available commands": {
        "id": "help"
      }
    },
    "Demos": {
      "Not a virus": {
        "onclick": "apps.open('Not a virus')"
      },
      "Dialogs": {
        "onclick": "apps.open('Dialogs')"
      },
      "UI Gallery": {
        "onclick": "apps.open('UI Gallery')"
      }
    }
  },
  "import": [
    "menu"
  ]
}