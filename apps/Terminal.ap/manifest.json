{
  "name": "Terminal",
  "version": "1.0.0",
  "about": "Basic terminal app",
  "sbin": true,
  "menus": {
    "Quick": {
      "Refresh taskbar": {
        "id": "command(\"app list\")"
      },
      "Check network": {
        "id": "command(\"net\")"
      },
      "View processes": {
        "id": "command(\"top\")"
      },
      "Clear past output": {
        "id": "command(\"clear\")"
      },
      "Available commands": {
        "id": "command(\"help\")"
      }
    },
    "Demos": {
      "Not a virus": {
        "onclick": "apps.open(\"Not a virus\")"
      },
      "Dialogs": {
        "onclick": "apps.open(\"Dialogs\")"
      },
      "UI Gallery": {
        "onclick": "apps.open(\"UI Gallery\")"
      }
    }
  },
  "import": [
    "menu"
  ]
}